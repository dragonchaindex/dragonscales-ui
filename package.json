{
  "name": "@dragonchaindex/toolkit",
  "version": "1.00.6",
  "description": "Set of UI components for dragonchain projects",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/dragonchaindex/dragonscales-ui",
  "license": "MIT",
   "scripts": {
    "start": "yarn storybook",
    "build": "rm -rf ./dist && rollup -c && tsc -d --emitDeclarationOnly --declarationDir dist",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check --loglevel error 'src/**/*.{js,jsx,ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest",
    "prepublishOnly": "npm build"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@rollup/plugin-typescript": "^8.3.0",
    "@rollup/plugin-url": "^6.1.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.3.1",
    "@types/styled-components": "^5.1.15",
    "babel-loader": "^8.2.2",
    "jest-styled-components": "^7.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.3.0",
    "rollup": "^2.58.0",
    "styled-components": "^5.1.7",
    "themeprovider-storybook": "^1.7.2",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.3"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "@types/lodash": "^4.14.168",
    "@types/styled-system": "^5.1.10",
    "build": "^0.1.4",
    "lodash": "^4.17.20",
    "react-popper": "^2.2.5",
    "storybook": "^6.3.12",
    "styled-system": "^5.1.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
